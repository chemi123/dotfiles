set number
set showmatch
set tabstop=4
set expandtab
set shiftwidth=4
set encoding=utf-8
set noerrorbells
set autoindent
syntax on
set scrolloff=5
filetype plugin indent on

hi DiffAdd    ctermfg=black ctermbg=2
hi DiffChange ctermfg=black ctermbg=3
hi DiffDelete ctermfg=black ctermbg=6
hi DiffText   ctermfg=black ctermbg=7
